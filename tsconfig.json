{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
        "strict": true,
        "baseUrl": "./",
        "paths": {
            "@appStack/*": ["src/screens/AppStack/*"],
            "@authStack/*": ["src/screens/AuthStack/*"],
            "@navigationStack/*": ["src/screens/Navigation/*"],
            "@components/*": ["src/components/*"],
            "@redux/*": ["src/redux/*"],
            "@utils/*": ["src/utils/*"]
        }
    }
}
